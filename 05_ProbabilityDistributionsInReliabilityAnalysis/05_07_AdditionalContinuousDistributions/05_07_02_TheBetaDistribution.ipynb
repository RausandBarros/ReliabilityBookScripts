   "source": [
    "x<-seq(0, 1, length=300) # Set the values for the x-axis\n",
    "# Set the parameters a (=alpha) and b (=beta)\n",
    "a<-2\n",
    "b<-5\n",
    "# Calculate the beta density y for each x\n",
    "y<-dbeta(x, a, b, log=FALSE)\n",
    "plot(x, y, type=\"l\", xlab=\"x\", ylab=\"f(x)\")"
   ]
